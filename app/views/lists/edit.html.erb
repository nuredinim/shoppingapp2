<h1>Edit <%= @list.name %> <%= @list.store %> <%= @list.date %></h1>
<form action="/lists/<%= @list.id %> " method="post">
	<input type="hidden" name="_method" value="patch">
	<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
	<p>
		<label>Name:</label>
		<input type="text" name="list[name]" value="<%= @list.name %>">
	</p>
	<p>
		<label>Store:</label>
		<input type="text" name="list[store]" value="<%= @list.store %>">
	</p>
	<p>
		<label>Date:</label>
		<input type="text" name="list[date]" value="<%= @list.date %>">
	</p>
	<input type="submit" name="Update">
</form>

<% if flash[:errors] %>
	<% flash[:errors].each do |e| %>
	<p><%= e %></p>
	<% end %>
<% end %>